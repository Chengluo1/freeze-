<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.edu.zjut.po.ExamplePanorama" table="examplepanorama" catalog="freeze">
		<id name="panorama1" type="string">
			<column name="panorama" length="255" />
			<generator class="assigned" />
		</id>

		<!-- 与example的多对一关联 -->
		<many-to-one name="example" class="cn.edu.zjut.po.Example" cascade="merge" lazy="false">
			<column name="exampleID" />
		</many-to-one>
	</class>
</hibernate-mapping>
